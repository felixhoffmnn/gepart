{
  "cSpell.language": "en, de-de, lorem-ipsum, python",
  "cSpell.words": [
    "autofix",
    "autorefs",
    "dhbw",
    "DHBW",
    "docstrings",
    "fontawesome",
    "Hoffmann",
    "isort",
    "kontakt",
    "linkedin",
    "mkdocs",
    "mkdocstrings",
    "mypy",
    "pytest",
    "pytests",
    "pyupgrade",
    "Studienarbeit",
    "wontfix"
  ],
  "yaml.schemas": {
    "https://squidfunk.github.io/mkdocs-material/schema.json": "mkdocs.yaml"
  },
  "cSpell.ignorePaths": [".git", ".vscode"],
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[python]": {
    "editor.defaultFormatter": null
  },
  "python.formatting.provider": "black",
  "gitmoji.outputType": "emoji"
}
